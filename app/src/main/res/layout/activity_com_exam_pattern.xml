<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ComExamPattern">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintDescription1"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">

        <ImageView
            android:id="@+id/imageRectangle2"
            android:layout_width="417dp"
            android:layout_height="82dp"
            android:scaleType="fitXY"
            android:src="@drawable/img_rectangle2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.12"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintGroup199"
            android:layout_width="367dp"
            android:layout_height="67dp"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginBottom="@dimen/_13pxv"
            app:layout_constraintBottom_toBottomOf="@+id/imageRectangle2"
            app:layout_constraintEnd_toEndOf="@+id/imageRectangle2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageRectangle2"
            app:layout_constraintTop_toTopOf="@+id/imageRectangle2"
            app:layout_constraintVertical_bias="0.0">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintGroup211"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_9pxv"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/txtDETAILS"
                    style="@style/txtUbuntubold20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_details"
                    app:layout_constraintBottom_toBottomOf="@+id/imageMenu"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.19"
                    app:layout_constraintStart_toEndOf="@+id/imageMenu"
                    app:layout_constraintTop_toTopOf="@+id/imageMenu"
                    tools:text="@string/lbl_details" />

                <ImageView
                    android:id="@+id/imageMenu"
                    android:layout_width="@dimen/_42pxh"
                    android:layout_height="@dimen/_47pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_menu"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/txtDETAILS"
                    app:layout_constraintHorizontal_bias="0.14"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="65dp"
                android:layout_height="53dp"
                android:src="@drawable/img_profile31"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.981"
                app:layout_constraintStart_toEndOf="@+id/constraintGroup211"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintGroup200"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="312dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintGroup199"
            app:layout_constraintVertical_bias="0.832">

            <TextView
                android:id="@+id/txtDescription"
                style="@style/txtSolidRoundedOutline_1"
                android:layout_width="159dp"
                android:layout_height="64dp"
                android:layout_marginStart="@dimen/_34pxh"
                android:gravity="center"
                android:paddingStart="@dimen/_10pxh"
                android:paddingTop="@dimen/_16pxv"
                android:paddingBottom="@dimen/_15pxv"
                android:text="@string/lbl_description"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/txtTestPattern"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0"
                tools:text="@string/lbl_description" />

            <TextView
                android:id="@+id/txtTestPattern"
                style="@style/txtSolidRoundedOutline_2"
                android:layout_width="164dp"
                android:layout_height="61dp"
                android:layout_marginStart="@dimen/_36pxh"
                android:layout_marginEnd="@dimen/_29pxh"
                android:gravity="center"
                android:paddingStart="@dimen/_16pxh"
                android:paddingTop="@dimen/_11pxv"
                android:paddingBottom="@dimen/_24pxv"

                android:text="@string/lbl_test_pattern"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/txtDescription"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="1.0"
                tools:text="@string/lbl_test_pattern" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtPatternForThe"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_60pxh"
            android:layout_marginStart="@dimen/_60pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="0dp" android:singleLine="true"
            android:text="@string/msg_pattern_for_the"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintGroup200"
            style="@style/txtUbuntubold20_1" tools:text="@string/msg_pattern_for_the"/>

        <TextView
            android:id="@+id/txtAptitudeTechn"
            style="@style/txtUbuntubold15"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_60pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_marginEnd="@dimen/_60pxh"
            android:layout_marginBottom="@dimen/_11pxv"
            android:gravity="start"
            android:text="@string/msg_aptitude_techn"
            app:layout_constraintBottom_toBottomOf="@+id/txtAPPLY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtPatternForThe"
            app:layout_constraintVertical_bias="0.072"
            tools:text="@string/msg_aptitude_techn" />

        <ImageView
            android:id="@+id/imageRectangle242"
            android:layout_width="177dp"
            android:layout_height="67dp"
            android:layout_marginStart="@dimen/_36pxh"
            android:layout_marginTop="@dimen/_122pxv"
            android:layout_marginEnd="@dimen/_33pxh"
            android:layout_marginBottom="@dimen/_71pxv"
            android:scaleType="fitXY"
            android:src="@drawable/img_rectangle242"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/txtAptitudeTechn"
            app:layout_constraintHorizontal_bias="0.478"
            app:layout_constraintStart_toStartOf="@+id/txtAptitudeTechn"
            app:layout_constraintTop_toTopOf="@+id/txtAptitudeTechn"
            app:layout_constraintVertical_bias="0.088" />

        <TextView
            android:id="@+id/txtAPPLY"
            style="@style/txtUbuntubold20"
            android:layout_width="73dp"
            android:layout_height="35dp"
            android:layout_marginStart="@dimen/_54pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_64pxh"
            android:layout_marginBottom="@dimen/_11pxv"
            android:ellipsize="end"
            android:gravity="center"
            android:singleLine="true"
            android:text="@string/lbl_apply"
            app:layout_constraintBottom_toBottomOf="@+id/imageRectangle242"
            app:layout_constraintEnd_toEndOf="@+id/imageRectangle242"
            app:layout_constraintHorizontal_bias="0.357"
            app:layout_constraintStart_toStartOf="@+id/imageRectangle242"
            app:layout_constraintTop_toTopOf="@+id/imageRectangle242"
            app:layout_constraintVertical_bias="0.111" />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="392dp"
            android:layout_height="162dp"
            android:layout_marginBottom="436dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.157"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageRectangle2"
            app:layout_constraintVertical_bias="0.755"
            app:srcCompat="@drawable/img_gate33" />

        <TextView
            android:id="@+id/textView3"
            style="@style/txtUbuntubold20"
            android:layout_width="261dp"
            android:layout_height="56dp"
            android:background="@drawable/border"
            android:gravity="center"
            android:text="@string/lbl_view"
            android:textColor="@color/black"
            app:layout_constraintBottom_toTopOf="@+id/imageRectangle242"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/txtAptitudeTechn"
            app:layout_constraintVertical_bias="0.422" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>