<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Exammat2">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup197"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">

        <ScrollView
            android:id="@+id/scrollViewScrollview"
            android:layout_width="408dp"
            android:layout_height="607dp"
            android:layout_marginTop="@dimen/_28pxv"
            android:clipToPadding="false"
            android:fillViewport="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageRectangle2">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintGroup189"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <View
                    android:id="@+id/viewRectangle265"
                    android:layout_width="@dimen/_414pxh"
                    android:layout_height="@dimen/_207pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:background="@color/white_A700"
                    app:layout_constraintEnd_toEndOf="@+id/imageImage3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <ImageView
                    android:id="@+id/imageImage3"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_207pxh"
                    android:scaleType="fitXY"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:src="@drawable/img_image3" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintGroup191"
                    style="@style/groupStylewhite_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_53pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/viewRectangle265" />

                <androidx.recyclerview.widget.RecyclerView
                    itemSpace="@{@dimen/_16pxh}"
                    android:layout_width="400dp"
                    android:layout_height="399dp"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    tools:ignore="MissingConstraints"
                    tools:itemCount="1"
                    tools:listitem="@layout/materials2" />
            </androidx.constraintlayout.widget.ConstraintLayout>


        </ScrollView>

        <ImageView
            android:id="@+id/imageRectangle2"
            android:layout_width="415dp"
            android:layout_height="99dp"
            android:scaleType="fitXY"
            android:src="@drawable/img_rectangle2_3"
            app:layout_constraintBottom_toTopOf="@+id/scrollViewScrollview"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.12"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent"
            app:contentInsetStart="0dp"
            app:layout_constraintEnd_toEndOf="@+id/imageRectangle2"
            app:layout_constraintStart_toStartOf="@+id/imageRectangle2" app:layout_constraintTop_toTopOf="@+id/imageRectangle2">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintGroup184"
                android:layout_height="match_parent" android:layout_width="match_parent">

                <ImageView
                    android:id="@+id/imageMenu"
                    android:layout_width="48dp"
                    android:layout_height="49dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_menu"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/txtMATERIALS"
                    app:layout_constraintHorizontal_bias="0.05"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <TextView
                    android:id="@+id/txtMATERIALS"
                    style="@style/txtUbuntubold20"
                    android:layout_width="311dp"
                    android:layout_height="32dp"
                    android:layout_marginStart="@dimen/_20pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:ellipsize="end"
                    android:gravity="start"
                    android:singleLine="true"
                    android:text="@{examMaterial2VM.examMaterial2Model.txtMATERIALS}"
                    app:layout_constraintBottom_toBottomOf="@+id/imageMenu"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageMenu"
                    app:layout_constraintTop_toTopOf="@+id/imageMenu"
                    app:layout_constraintVertical_bias="0.0"
                    tools:text="@string/lbl_materials" />

                <ImageView
                    android:id="@+id/imageProfile2"
                    android:layout_width="67dp"
                    android:layout_height="44dp"
                    android:scaleType="fitXY"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="318dp"
                    tools:layout_editor_absoluteY="0dp"
                    tools:src="@drawable/img_profile31" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.appcompat.widget.Toolbar>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>