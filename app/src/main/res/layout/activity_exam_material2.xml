                            android:gravity="start"
                            android:id="@+id/txtLESSON3"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_23pxv"
                            android:layout_marginEnd="@dimen/_74pxh"
                            android:layout_marginStart="@dimen/_46pxh"
                            android:layout_marginTop="@dimen/_24pxv"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{examMaterial2VM.examMaterial2Model.txtLESSON3}"
                            app:layout_constraintBottom_toBottomOf="@+id/imageImage7"
                            app:layout_constraintEnd_toStartOf="@+id/imageImage7"
