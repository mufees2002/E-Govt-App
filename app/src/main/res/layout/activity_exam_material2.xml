                        <ImageView android:id="@+id/imageImage6"
                            android:layout_height="@dimen/_74pxh"
                            android:layout_marginBottom="@dimen/_12pxv"
                            android:layout_marginEnd="@dimen/_32pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_width="@dimen/_89pxh"
                            android:scaleType="fitXY"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.24"
                            app:layout_constraintStart_toEndOf="@+id/txtLESSON2"
                            app:layout_constraintTop_toTopOf="parent"
                            imageRes="@{@drawable/img_image5}" tools:src="@drawable/img_image5"/>
                    </androidx.constraintlayout.widget.ConstraintLayout>
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/constraintGroup187"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_48pxv"
                        android:layout_marginEnd="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_25pxv"
                        android:layout_width="0dp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/constraintGroup186" style="@style/groupStylewhite_3">
