    <data>
        <variable name="examMaterial2VM" type="com.application.app.modules.exammaterial2.data.viewmodel.ExamMaterial2VM"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup197"
