<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="jobsPortal1RowModel" type="com.application.app.modules.jobsportal.data.model.JobsPortal1RowModel"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_margin="@dimen/_10pxh" style="@style/groupStylewhite_1">
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtNAMEOFTHEJOB"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_55pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_width="0dp" android:singleLine="true"
            android:text="@{jobsPortal1RowModel.txtNAMEOFTHEJOB}"
            app:layout_constraintBottom_toTopOf="@+id/txtLOCATION"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/viewRectangle243"
            app:layout_constraintTop_toTopOf="@+id/viewRectangle243"
            style="@style/txtUbuntubold20_1" tools:text="@string/msg_name_of_the_job"/>
        <View android:background="@color/gray_400"
            android:id="@+id/viewRectangle243"
            android:layout_height="@dimen/_72pxv"
            android:layout_marginBottom="@dimen/_24pxv"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_width="@dimen/_74pxh"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/txtNAMEOFTHEJOB"
            app:layout_constraintHorizontal_bias="0.12"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toTopOf="parent"/>
        <TextView android:ellipsize="end" android:id="@+id/txtLOCATION"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_24pxv"
            android:layout_marginEnd="@dimen/_144pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{jobsPortal1RowModel.txtLOCATION}"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.03"
            app:layout_constraintStart_toEndOf="@+id/viewRectangle243"
            app:layout_constraintTop_toBottomOf="@+id/txtNAMEOFTHEJOB"
            style="@style/txtUbuntubold20_1" tools:text="@string/lbl_location"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
