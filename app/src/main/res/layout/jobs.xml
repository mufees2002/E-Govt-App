<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintGroup"
        style="@style/groupStylewhite_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_10pxh"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="27dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/txtNAMEOFTHEJOB"
            style="@style/txtUbuntubold20_1"
            app:layout_constraintBottom_toTopOf="@+id/txtLOCATION"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/viewRectangle243"
            app:layout_constraintTop_toTopOf="@+id/viewRectangle243"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_55pxh"
            android:ellipsize="end"
            android:gravity="start"
            android:singleLine="true"
            tools:text="@string/msg_name_of_the_job" />

        <ImageView
            android:id="@+id/viewRectangle243"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/txtNAMEOFTHEJOB"
            app:layout_constraintHorizontal_bias="0.12"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="@dimen/_74pxh"
            android:layout_height="@dimen/_72pxv"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_27pxv"
            android:layout_marginBottom="@dimen/_24pxv"
            android:background="@color/gray_400" />

        <TextView
            android:id="@+id/txtLOCATION"
            style="@style/txtUbuntubold20_1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.03"
            app:layout_constraintStart_toEndOf="@+id/viewRectangle243"
            app:layout_constraintTop_toBottomOf="@+id/txtNAMEOFTHEJOB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_marginEnd="@dimen/_144pxh"
            android:layout_marginBottom="@dimen/_24pxv"
            android:ellipsize="end"
            android:singleLine="true"
            tools:text="@string/lbl_location" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>