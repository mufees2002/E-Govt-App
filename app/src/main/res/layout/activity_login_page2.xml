<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLoginPage1"
    android:layout_height="match_parent"
    android:layout_width="match_parent" style="@style/groupStylewhite_2">

    <TextView
        android:id="@+id/txtLogin"
        style="@style/txtIntersemibold30"
        android:layout_width="125dp"
        android:textColor="@color/black"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/_172pxh"
        android:layout_marginTop="@dimen/_28pxv"
        android:layout_marginEnd="@dimen/_170pxh"
        android:ellipsize="end"
        android:gravity="center"
        android:singleLine="true"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@string/lbl_login" />


    <EditText
        android:id="@+id/etEmail"
        style="@style/etIntermedium16"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8pxh"
        android:layout_marginTop="@dimen/_82pxv"
        android:layout_marginEnd="@dimen/_9pxh"
        android:background="@color/gray_100"
        android:ellipsize="end"
        android:gravity="start"
        android:hint="@string/lbl_email"
        android:paddingStart="@dimen/_16pxh"
        android:paddingTop="@dimen/_17pxv"
        android:paddingBottom="@dimen/_16pxv"
        android:shadowColor="@color/black"
        android:singleLine="true"

        android:textColor="@color/black"
        android:textColorHint="@color/gray_400"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtLogin"
        app:layout_constraintVertical_bias="0.0" />



    <EditText
        android:id="@+id/etEmail1"
        style="@style/etIntermedium16"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8pxh"
        android:layout_marginTop="164dp"
        android:layout_marginEnd="@dimen/_9pxh"
        android:background="@color/gray_100"
        android:ellipsize="end"
        android:gravity="start"
        android:inputType="textPassword"
        android:hint="@string/lbl_password"
        android:paddingStart="@dimen/_16pxh"
        android:paddingTop="@dimen/_17pxv"
        android:paddingBottom="@dimen/_16pxv"
        android:shadowColor="@color/black"
        android:singleLine="true"

        android:textColor="@color/black"
        android:textColorHint="@color/gray_400"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtLogin"
        app:layout_constraintVertical_bias="0.029" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnLogIn"
        style="@style/btnSolidRounded"
        android:layout_width="0dp"
        android:layout_height="@dimen/_51pxv"
        android:layout_marginStart="@dimen/_10pxh"
        android:layout_marginEnd="@dimen/_7pxh"
        android:ellipsize="end"
        android:gravity="center"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_16pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_16pxv"
        android:singleLine="true"
        android:text="@string/lbl_log_in2"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:text="@string/lbl_log_in2" />

    <TextView
        android:id="@+id/txtForgotYourPas1"
        style="@style/txtIntersemibold16"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_133pxh"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="@dimen/_112pxh"
        android:ellipsize="end"
        android:gravity="start"
        android:singleLine="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnLogIn"
        app:layout_constraintVertical_bias="0.106"
        tools:text="@string/msg_forgot_your_pas" />

    <ImageView android:id="@+id/imageRectangle12"
        android:layout_height="@dimen/_80pxh"
        android:layout_marginBottom="@dimen/_29pxv"
        android:layout_marginEnd="@dimen/_30pxh"
        android:layout_marginStart="@dimen/_14pxh"
        android:layout_marginTop="@dimen/_28pxv"
        android:layout_width="@dimen/_329pxh"
        android:scaleType="fitXY"
        android:src="@drawable/img_rectangle12"
        app:layout_constraintBottom_toBottomOf="@+id/imageImage1"
        app:layout_constraintEnd_toEndOf="@+id/txtCONTINUEWITHH"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="@+id/imageImage1" app:layout_constraintTop_toTopOf="@+id/imageImage1"/>
    <TextView android:id="@+id/txtCONTINUEWITHH"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/_15pxv"
        android:layout_marginEnd="@dimen/_22pxh"
        android:layout_marginStart="@dimen/_95pxh"
        android:layout_marginTop="@dimen/_26pxv"
        android:layout_width="wrap_content"
        android:text="@string/msg_continue_with_h"
        app:layout_constraintBottom_toBottomOf="@+id/imageRectangle12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.32"
        app:layout_constraintStart_toStartOf="@+id/imageRectangle12"
        app:layout_constraintTop_toTopOf="@+id/imageRectangle12"
        style="@style/txtUbuntubold15" tools:text="@string/msg_continue_with_h"/>
    <ImageView android:id="@+id/imageImage1"
        android:layout_height="@dimen/_137pxh"
        android:layout_marginBottom="@dimen/_28pxv"
        android:layout_marginEnd="@dimen/_219pxh"
        android:layout_marginStart="@dimen/_28pxh"
        android:layout_marginTop="@dimen/_22pxv"
        android:layout_width="@dimen/_124pxh"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageRectangle12"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtSignUp1"
        android:src="@drawable/img_image1"/>

    <TextView
        android:id="@+id/txtLogin4"
        style="@style/txtIntersemibold16"
        android:layout_width="57dp"
        android:layout_height="30dp"
        android:ellipsize="end"
        android:gravity="center"
        android:singleLine="true"

        android:text="@string/lbl_show"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/etEmail1"
        app:layout_constraintEnd_toEndOf="@+id/etEmail1"
        app:layout_constraintHorizontal_bias="0.937"
        app:layout_constraintStart_toStartOf="@+id/etEmail1"
        app:layout_constraintTop_toTopOf="@+id/etEmail1" />
</androidx.constraintlayout.widget.ConstraintLayout>