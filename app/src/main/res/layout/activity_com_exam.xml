<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ComExam">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintTestpattern1"
        style="@style/groupStylewhite_A700"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="126dp">

        <ImageView
            android:id="@+id/imageRectangle2"
            android:layout_width="412dp"
            android:layout_height="79dp"
            android:scaleType="fitXY"
            android:src="@drawable/img_rectangle2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.12"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintGroup224"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginBottom="@dimen/_13pxv"
            app:layout_constraintBottom_toBottomOf="@+id/imageRectangle2"
            app:layout_constraintEnd_toEndOf="@+id/imageRectangle2"
            app:layout_constraintStart_toStartOf="@+id/imageRectangle2"
            app:layout_constraintTop_toTopOf="@+id/imageRectangle2">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintGroup242"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_17pxv"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/txtDETAILS"
                    style="@style/txtUbuntubold20"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_19pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_details"
                    app:layout_constraintBottom_toBottomOf="@+id/imageMenu"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.14"
                    app:layout_constraintStart_toEndOf="@+id/imageMenu"
                    app:layout_constraintTop_toTopOf="@+id/imageMenu"
                    app:layout_constraintVertical_bias="0.0"
                    tools:text="@string/lbl_details" />

                <ImageView
                    android:id="@+id/imageMenu"
                    android:layout_width="@dimen/_42pxh"
                    android:layout_height="@dimen/_47pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_menu"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/txtDETAILS"
                    app:layout_constraintHorizontal_bias="0.1"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="65dp"
                android:layout_height="53dp"
                android:layout_marginStart="276dp"
                android:src="@drawable/img_profile31"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.466"
                app:layout_constraintStart_toStartOf="@+id/constraintGroup242"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.529" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintGroup225"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageRectangle2"
            app:layout_constraintVertical_bias="0.35"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/imageRectangle22"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="@dimen/_36pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_rectangle22_1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/txtTestPattern"
                app:layout_constraintHeight_percent="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                app:layout_constraintWidth_percent="0.38297874" />

            <TextView
                android:id="@+id/txtDescription"
                style="@style/txtUbuntubold20"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_marginBottom="@dimen/_15pxv"
                android:ellipsize="end"
                android:gravity="center"
                android:singleLine="true"
                android:text="@string/lbl_description"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/imageRectangle22"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="@+id/imageRectangle22"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0"
                tools:text="@string/lbl_description" />

            <TextView
                android:id="@+id/txtTestPattern"
                style="@style/txtSolidRoundedOutline_1"
                android:layout_width="154dp"
                android:layout_height="68dp"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginEnd="@dimen/_42pxh"
                android:gravity="center"
                android:paddingStart="@dimen/_14pxh"
                android:paddingTop="@dimen/_16pxv"
                android:paddingBottom="@dimen/_19pxv"
                android:text="@string/lbl_test_pattern"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.11"
                app:layout_constraintStart_toEndOf="@+id/imageRectangle22"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="@string/lbl_test_pattern" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <TextView
            android:id="@+id/txtAboutTheExam"
            style="@style/txtUbuntubold20_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_53pxh"
            android:layout_marginTop="@dimen/_49pxv"
            android:layout_marginEnd="@dimen/_53pxh"
            android:ellipsize="end"
            android:gravity="start"
            android:singleLine="true"
            android:text="@string/lbl_about_the_exam"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintGroup225"
            tools:text="@string/lbl_about_the_exam" />

        <TextView
            android:id="@+id/txtGATEGraduate"
            style="@style/txtUbuntubold15_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_44pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginBottom="@dimen/_44pxv"
            android:gravity="start"
            android:text="@string/msg_gate_graduate"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtAboutTheExam"
            app:layout_constraintVertical_bias="0.326"
            tools:text="@string/msg_gate_graduate" />

        <ImageView
            android:id="@+id/imageRectangle242"
            android:layout_width="161dp"
            android:layout_height="67dp"
            android:layout_marginStart="@dimen/_58pxh"
            android:layout_marginTop="@dimen/_141pxv"
            android:layout_marginEnd="@dimen/_98pxh"
            android:layout_marginBottom="@dimen/_44pxv"
            android:scaleType="fitXY"
            android:src="@drawable/img_rectangle242_2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/txtGATEGraduate"
            app:layout_constraintHorizontal_bias="0.214"
            app:layout_constraintStart_toStartOf="@+id/txtGATEGraduate"
            app:layout_constraintTop_toTopOf="@+id/txtGATEGraduate"
            app:layout_constraintVertical_bias="0.434" />

        <TextView
            android:id="@+id/txtAPPLY2"
            style="@style/txtUbuntubold20"
            android:layout_width="73dp"
            android:layout_height="35dp"
            android:layout_marginStart="@dimen/_54pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_64pxh"
            android:layout_marginBottom="@dimen/_11pxv"
            android:ellipsize="end"
            android:gravity="center"
            android:singleLine="true"
            android:text="@string/lbl_apply"
            app:layout_constraintBottom_toBottomOf="@+id/imageRectangle242"
            app:layout_constraintEnd_toEndOf="@+id/imageRectangle242"
            app:layout_constraintHorizontal_bias="0.352"
            app:layout_constraintStart_toStartOf="@+id/imageRectangle242"
            app:layout_constraintTop_toTopOf="@+id/imageRectangle242"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/txtAPPLY"
            style="@style/txtUbuntubold20"
            android:layout_width="78dp"
            android:layout_height="23dp"
            android:layout_marginStart="@dimen/_49pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_69pxh"
            android:layout_marginBottom="@dimen/_11pxv"
            android:ellipsize="end"
            android:gravity="center"
            android:singleLine="true"
            android:text="@string/lbl_apply"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@+id/txtGATEGraduate"
            app:layout_constraintEnd_toEndOf="@+id/imageRectangle242"
            app:layout_constraintHorizontal_bias="0.153"
            app:layout_constraintStart_toStartOf="@+id/imageRectangle242"
            app:layout_constraintTop_toTopOf="@+id/imageRectangle242"
            app:layout_constraintVertical_bias="0.0"
            tools:text="@string/lbl_apply" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="378dp"
            android:layout_height="150dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.363"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintGroup224"
            app:layout_constraintVertical_bias="0.085"
            app:srcCompat="@drawable/img_gate31" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>